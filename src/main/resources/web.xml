<web-app>
    <servlet>
        <servlet-name>RLA Reservation REST Service</servlet-name>
        <servlet-class>com.sun.jersey.spi.container.servlet.ServletContainer</servlet-class>
        <init-param>
            <param-name>com.sun.jersey.config.property.packages</param-name>
            <param-value>com.vmware.q3team7</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>RLA Data REST Service</servlet-name>
        <url-pattern>/api/v1/*</url-pattern>
    </servlet-mapping>
</web-app>